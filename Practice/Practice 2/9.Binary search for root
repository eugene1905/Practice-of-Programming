#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
double f(double x){
    return pow(x,5) - 15 * pow(x,4) + 85 * pow(x,3) - 225 * pow(x,2) + 274 * x - 121;
}
int main(){
    double a = 1.5, b = 2.4, eps = 1e-6;
    double root;
    while(true){
        root = a + (b - a) / 2;
        double y = f(root);
        if(abs(y) < eps)break;
        if(y > 0)
            a = root;
        else
            b = root;
    }
    cout << fixed << setprecision(6) << root << endl;
}
